{% extends 'base.html.twig' %}

{% block body %}
    <h1>Toutes les recettes en moins d'une heure</h1>
    {% for recipe in recipes %}
        <ul>
            <li>
                <a href="{{ url('recipe.show', {id: recipe.id, slug: recipe.slug}) }}">{{ recipe.title }}</a>
            </li>
        </ul>
    {%  endfor %}
{% endblock %}
